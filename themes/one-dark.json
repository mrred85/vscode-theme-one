{
	"$schema": "vscode://schemas/color-theme",
	"name": "One Dark",
	"type": "dark",
	"colors": {
		"dropdown.background": "#2c323c",
		"editor.background": "#282c34",
		"editor.foreground": "#abb2bf",
		"editor.selectionBackground": "#3e4451",
		"editor.selectionHighlightBackground": "#414352",
		"editor.lineHighlightBackground": "#2c323c",
		"editor.lineHighlightBorder": "#2c323c",
		"editor.findMatchBackground": "#d6e4fe",
		"editor.findMatchHighlightBackground": "#986801",
		"editor.findRangeHighlightBackground": "#d6e4fe",
		"editorCursor.foreground": "#528bff",
		"editorWhitespace.foreground": "#747369",
		"editorIndentGuide.background": "#414352",
		"editorLineNumber.foreground": "#4a5364",
		"editorLink.activeForeground": "#61afef",
		"editorGroupHeader.tabsBackground": "#20242a",
		"editorGroupHeader.noTabsBackground": "#20242a",
		"editorGroup.dropBackground": "#2c323c",
		"editorGroup.border": "#1b1e24",
		"tab.border": "#20242a",
		"tab.activeBackground": "#282c34",
		"tab.activeForeground": "#c5d2ec",
		"tab.inactiveBackground": "#20242a",
		"tab.inactiveForeground": "#6b707e",
		"activityBar.background": "#20242a",
		"activityBar.dropBackground": "#2c323c",
		"activityBar.foreground": "#abb2bf", //6b707e
		"activityBarBadge.background": "#3371f1",
		"activityBarBadge.foreground": "#d6e4fe",

		"sideBar.background": "#20242a",
		"sideBarSectionHeader.background": "#3371f1",
		"sideBarTitle.foreground": "#d6e4fe",
		"button.background": "#3371f1",
		"button.foreground": "#d6e4fe",
		"button.hoverBackground": "#528bff",

		"input.background": "#282c34",
		"input.border": "#4a5364", //bebebe //2c323c
		"input.foreground": "#c5d2ec",
		"inputOption.activeBorder": "#61afef"
	},
	"tokenColors": [
		{
			"settings": {
				"background": "#282c34",
				"foreground": "#abb2bf"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#61afef"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#5c6370",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"constant.language",
				"constant.other",
				"support.constant.core",
				"support.constant.ext",
				"support.constant.std"
			],
			"settings": {
				"foreground": "#d19a66"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#d19a66"
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#56b6c2"
			}
		},
		{
			"name": "TAG elements",
			"scope": [
				"entity.name.tag",
				"entity.name.tag.css",
				"entity.name.tag.scss",
				"entity.tag.apacheconf"
			],
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#d19a66"
			}
		},
		{
			"name": "CSS / SCSS / HTML CSS attribute name",
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.class.mixin.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.pseudo-class.css",
				"entity.other.attribute-name.pseudo-element.css",
				"entity.other.attribute-name.attribute.scss",
				"entity.other.attribute-name.scss"
			],
			"settings": {
				"foreground": "#d19a66"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.id.css",
				"source.css.less entity.other.attribute-name.id"
			],
			"settings": {
				"foreground": "#61afef"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#A8292D"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d19a66"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#e06c75"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#c678dd"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#50a14f"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#df6a73"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#c678dd"
			}
		},
		{
			"scope": "beginning.punctuation.definition.quote.markdown",
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"scope": "beginning.punctuation.definition.list.markdown",
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"scope": [
				// "punctuation.definition.metadata.markdown",
				"string.other.link.title.markdown",
				"string.other.link.description.markdown"
			],
			"settings": {
				"foreground": "#abb2bf"
			}
		},
		{
			"scope": [
				"markup.underline.link.markdown",
				"markup.underline.link.image.markdown"
			],
			"settings": {
				"foreground": "#c678dd"
			}
		},
		{
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#50a14f"
			}
		},
		{
			"scope": "meta.selector",
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#abb2bf"
			}
		},
		{
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#56b6c2"
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#98c379"
			}
		},
		{
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#d19a66"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#c678dd"
			}
		},
		{
			"scope": [
				"storage",
				"storage.type",
				"storage.modifier",
				"storage.modifier.import.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#c678dd"
			}
		},
		{
			"scope": [
				"string",
				"string.tag",
				"string.value"
			],
			"settings": {
				"foreground": "#98c379"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#56b6c2"
			}
		},
		{
			"name": "JavaScript string interpolation ${}",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end"
			],
			"settings": {
				"foreground": "#e5c07b"
			}
		},
		{
			"name": "JavaScript string interpolation ${var}",
			"scope": "meta.template.expression.variable",
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"scope": "support.type.property-name",
			"settings": {
				"foreground": "#df6a73"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#c678dd"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#c678dd"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#abb2bf"
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression"
			],
			"settings": {
				"foreground": "#c678dd"
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#d19a66"
			}
		},
		{
			"scope": "constant.sha.git-rebase",
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"name": "this.self",
			"scope": "variable.language",
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"java.class.entity.name.function"
			],
			"settings": {
				"foreground": "#61afef"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"support.function",
				"support.function.git-rebase"
			],
			"settings": {
				"foreground": "#56b6c2"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"meta.return-type",
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.class",
				"storage.type.cs",
				"storage.type.generic.cs",
				"storage.type.modifier.cs",
				"storage.type.variable.cs",
				"storage.type.annotation.java",
				"storage.type.generic.java",
				"storage.type.java",
				"storage.type.object.array.java",
				"storage.type.primitive.array.java",
				"storage.type.primitive.java",
				"storage.type.token.java",
				"storage.type.groovy",
				"storage.type.annotation.groovy",
				"storage.type.parameters.groovy",
				"storage.type.generic.groovy",
				"storage.type.object.array.groovy",
				"storage.type.primitive.array.groovy",
				"storage.type.primitive.groovy"
			],
			"settings": {
				"foreground": "#e5c07b"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"entity.other.class"
			],
			"settings": {
				"foreground": "#98c379"
			}
		},
		{
			"name": "Types declaration and references, TS grammar specific",
			"scope": [
				"meta.return.type",
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json"
			],
			"settings": {
				"foreground": "#e5c07b"
			}
		},
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable"
			],
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"name": "CSS / SCSS / HTML CSS text",
			"scope": [
				"meta.property-value.css support",
				"constant.other.color.rgb-value.hex.css punctuation.definition",

				"meta.property-value.scss support",
				"constant.numeric.color.hex-value.scss punctuation.definition"
			],
			"settings": {
				"foreground": "#d19a66"
			}
		},
		{
			"name": "CSS / SCSS / HTML CSS punctuation",
			"scope": [
				"support.type.property-name.css",
				"support.type.property-name.html.css",

				"support.type.property-name.scss"
			],
			"settings": {
				"foreground": "#abb2bf"
			}
		},
		{
			"name": "CSS / SCSS / HTML CSS RGB-HEX values",
			"scope": [
				"meta.property-value.css support.function",
				"constant.other.color.hex-value.css",
				"constant.other.color.rgb-value.hex.css",

				"meta.property-value.scss support.function",
				"constant.numeric.color.hex-value.scss"
			],
			"settings": {
				"foreground": "#56b6c2"
			}
		},
		{
			"name": "PHP tag <?php",
			"scope": [
				"punctuation.section.embedded.begin.metatag.php",
				"punctuation.section.embedded.end.metatag.php"
			],
			"settings": {
				"foreground": "#e06c75"
			}
		}
	]
}
