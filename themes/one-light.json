{
	"$schema": "vscode://schemas/color-theme",
	"name": "One Light",
	"type": "light",
	"colors": {
		"focusBorder": "#4078f2",
		"dropdown.background": "#f2f2f2",
		"dropdown.border": "#bebebe",
		"dropdown.foreground": "#424242",
		"editor.background": "#fafafa",
		"editor.foreground": "#383a42",
		"editor.selectionBackground": "#dedede",
		"editor.selectionHighlightBackground": "#d2d3d4",
		"editor.inactiveSelectionBackground": "#dedede",
		"editor.lineHighlightBackground": "#f2f2f2",
		"editor.lineHighlightBorder": "#f2f2f2",
		"editor.findMatchBackground": "#d6ddfe",
		"editor.findMatchHighlightBackground": "#e5c07b",
		"editor.findRangeHighlightBackground": "#d6ddfe",
		"editorCursor.foreground": "#526fff",
		"editorWhitespace.foreground": "#d5d6d7",
		"editorIndentGuide.background": "#d2d3d4",
		"editorLineNumber.foreground": "#bebebe",
		"editorLink.activeForeground": "#4078f2",
		"editorGroupHeader.tabsBackground": "#eaeaeb",
		"editorGroupHeader.noTabsBackground": "#eaeaeb",
		"editorGroup.dropBackground": "#f2f2f2",
		"editorGroup.border": "#dadbdc",
		"tab.border": "#eaeaeb",
		"tab.activeBackground": "#fafafa",
		"tab.activeForeground": "#424242",
		"tab.inactiveBackground": "#eaeaeb",
		"tab.inactiveForeground": "#8e8e90",
		"activityBar.background": "#eaeaeb",
		"activityBar.dropBackground": "#f2f2f2",
		"activityBar.foreground": "#424242",
		"activityBarBadge.background": "#3772ef",
		"activityBarBadge.foreground": "#d6ddfe",
		"sideBar.background": "#eaeaeb",
		"sideBarSectionHeader.background": "#d2d3d4",
		"sideBarTitle.foreground": "#424242",
		"button.background": "#3772ef",
		"button.foreground": "#d6e4fe",
		"button.hoverBackground": "#526fff",
		"input.background": "#fafafa",
		"input.border": "#bebebe",
		"input.foreground": "#424242",
		"inputOption.activeBorder": "#4078f2",
		"widget.shadow": "#bebebe",
		"editorWidget.background": "#eaeaeb",
		"editorSuggestWidget.background": "#fafafa",
		"editorSuggestWidget.border": "#bebebe",
		"editorSuggestWidget.foreground": "#424242",
		"editorSuggestWidget.highlightForeground": "#e06c75",
		"editorSuggestWidget.selectedBackground": "#d6ddfe",
		"editorHoverWidget.background": "#fafafa",
		"editorHoverWidget.border": "#bebebe",
		"panel.background": "#fafafa",
		"panel.border": "#d2d3d4",
		"panelTitle.activeBorder": "#4078f2",
		"panelTitle.activeForeground": "#e06c75",
		"panelTitle.inactiveForeground": "#8e8e90",
		"list.focusBackground": "#d6ddfe",
		"list.highlightForeground": "#e06c75",
		"list.activeSelectionBackground": "#d6ddfe",
		"list.activeSelectionForeground": "#424242",
		"list.inactiveSelectionBackground": "#dadbdc",
		"list.hoverBackground": "#d5d6d7",
		"notification.background": "#eaeaeb",
		"notification.foreground": "#424242",
		"debugToolBar.background": "#eaeaeb",
		"pickerGroup.border": "#4078f2",
		"pickerGroup.foreground": "#e06c75",
		"statusBar.background": "#eaeaeb",
		"statusBar.noFolderBackground": "#eaeaeb",
		"statusBar.debuggingBackground": "#eaeaeb",
		"statusBar.foreground": "#424242",
		"statusBarItem.hoverBackground": "#d6ddfe",
		"statusBarItem.activeBackground": "#e06c75",
		"titleBar.activeBackground": "#eaeaeb",
		"titleBar.activeForeground": "#424242",
		"titleBar.inactiveBackground": "#eaeaeb",
		"titleBar.inactiveForeground": "#8e8e90"
	},
	"tokenColors": [
		{
			"settings": {
				"background": "#fafafa",
				"foreground": "#383a42"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#4078f2"
			}
		},

		{
			"scope": [
				"comment.line",
				"comment.block",
				"comment.block.preprocessor",
				"comment.block.documentation",
				"comment.block.documentation entity.name.type.instance",
				"comment.block.documentation variable.other"
			],
			"settings": {
				"foreground": "#a0a1a7",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"constant.language",
				"constant.other",
				"support.constant.core",
				"support.constant.ext",
				"support.constant.std"
			],
			"settings": {
				"foreground": "#986801"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#986801"
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#0184bc"
			}
		},
		{
			"name": "TAG elements",
			"scope": [
				"entity.name.tag",
				"entity.name.tag.css",
				"entity.name.tag.scss",
				"entity.tag.apacheconf"
			],
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#986801"
			}
		},
		{
			"name": "CSS / SCSS / HTML CSS attribute name",
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.class.mixin.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.pseudo-class.css",
				"entity.other.attribute-name.pseudo-element.css",

				"entity.other.attribute-name.attribute.scss",
				"entity.other.attribute-name.scss"
			],
			"settings": {
				"foreground": "#986801"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.id.css",
				"source.css.less entity.other.attribute-name.id"
			],
			"settings": {
				"foreground": "#4078f2"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#A8292D"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#986801"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#e06c75"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#a626a4"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#90c378"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#df6a73"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#c679dd"
			}
		},
		{
			"scope": "beginning.punctuation.definition.quote.markdown",
			"settings": {
				"foreground": "#4078f2"
			}
		},
		{
			"scope": "beginning.punctuation.definition.list.markdown",
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"scope": [
				// "punctuation.definition.metadata.markdown",
				"string.other.link.title.markdown",
				"string.other.link.description.markdown"
			],
			"settings": {
				"foreground": "#383a42"
			}
		},
		{
			"scope": [
				"markup.underline.link.markdown",
				"markup.underline.link.image.markdown"
			],
			"settings": {
				"foreground": "#a626a4"
			}
		},
		{
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#90c378"
			}
		},
		{
			"scope": "meta.selector",
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#383a42"
			}
		},
		{
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#0184bc"
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#50a14f"
			}
		},
		{
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#986801"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#a626a4"
			}
		},
		{
			"scope": [
				"storage",
				"storage.type",
				"storage.modifier",
				"storage.modifier.import.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#a626a4"
			}
		},
		{
			"scope": [
				"string",
				"string.tag",
				"string.value"
			],
			"settings": {
				"foreground": "#50a14f"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#0184bc"
			}
		},
		{
			"name": "JavaScript string interpolation ${}",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end"
			],
			"settings": {
				"foreground": "#986801"
			}
		},
		{
			"name": "JavaScript string interpolation ${var}",
			"scope": "meta.template.expression.variable",
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"scope": "support.type.property-name",
			"settings": {
				"foreground": "#df6a73"
			}
		},
		{
			"scope": [
				"keyword",
				"keyword.control",
				"comment.line keyword.codetag.notation",
				"comment.block.documentation keyword.other",
				"comment.block.documentation storage.type.class"
			],
			"settings": {
				"foreground": "#a626a4"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#383a42"
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression"
			],
			"settings": {
				"foreground": "#a626a4"
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#986801"
			}
		},
		{
			"scope": "constant.sha.git-rebase",
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"name": "this.self",
			"scope": "variable.language",
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"java.class.entity.name.function"
			],
			"settings": {
				"foreground": "#4078f2"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"support.function",
				"support.function.git-rebase"
			],
			"settings": {
				"foreground": "#0184bc"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"meta.return-type",
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.class",

				"storage.type.cs",
				"storage.type.generic.cs",
				"storage.type.modifier.cs",
				"storage.type.variable.cs",

				"storage.type.annotation.java",
				"storage.type.generic.java",
				"storage.type.java",
				"storage.type.object.array.java",
				"storage.type.primitive.array.java",
				"storage.type.primitive.java",
				"storage.type.token.java",

				"storage.type.groovy",
				"storage.type.annotation.groovy",
				"storage.type.parameters.groovy",
				"storage.type.generic.groovy",
				"storage.type.object.array.groovy",
				"storage.type.primitive.array.groovy",
				"storage.type.primitive.groovy"
			],
			"settings": {
				"foreground": "#c18401"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#50a14f"
			}
		},
		{
			"name": "Types declaration and references, TS grammar specific",
			"scope": [
				"meta.return.type",
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json"
			],
			"settings": {
				"foreground": "#c18401"
			}
		},
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable"
			],
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"name": "CSS / SCSS / HTML CSS text",
			"scope": [
				"meta.property-value.css support",
				"constant.other.color.rgb-value.hex.css punctuation.definition",

				"meta.property-value.scss support",
				"constant.numeric.color.hex-value.scss punctuation.definition"
			],
			"settings": {
				"foreground": "#986801"
			}
		},
		{
			"name": "CSS / SCSS / HTML CSS punctuation",
			"scope": [
				"support.type.property-name.css",
				"support.type.property-name.html.css",

				"support.type.property-name.scss"
			],
			"settings": {
				"foreground": "#383a42"
			}
		},
		{
			"name": "CSS / SCSS / HTML CSS RGB-HEX values",
			"scope": [
				"meta.property-value.css support.function",
				"constant.other.color.hex-value.css",
				"constant.other.color.rgb-value.hex.css",

				"meta.property-value.scss support.function",
				"constant.numeric.color.hex-value.scss"
			],
			"settings": {
				"foreground": "#0184bc"
			}
		},
		{
			"name": "PHP tag <?php",
			"scope": [
				"punctuation.section.embedded.begin.metatag.php",
				"punctuation.section.embedded.end.metatag.php"
			],
			"settings": {
				"foreground": "#e06c75"
			}
		}
	]
}
