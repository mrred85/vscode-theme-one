{
	"name": "One Light",
	"settings": [
		{
			"settings": {
				"background": "#fafafa",
				"foreground": "#383a42",
				"caret": "#526fff",
				"lineHighlight": "#f2f2f2",
				"selection": "#ededed",
				"invisibles": "#d5d6d7",
				"guide": "#d2d3d4",
				"hoverHighlight": "#d2d3d4",
				"findRangeHighlight": "#d6ddfe",
				"findMatchHighlight": "#e5c07b",
				"currentFindMatchHighlight": "#d6ddfe",
				"activeLinkForeground": "#4078f2",
				"gotoDefinitionLinkForeground": "#4078f2"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#4078f2"
			}
		},

		{
			"scope": "comment",
			"settings": {
				"foreground": "#a0a1a7",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"constant.language",
				"constant.other",
				"support.constant.core",
				"support.constant.ext",
				"support.constant.std"
			],
			"settings": {
				"foreground": "#986801"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#986801"
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#0184bc"
			}
		},
		{
			"name": "TAG elements",
			"scope": [
				"entity.name.tag",
				"entity.name.tag.css",
				"entity.name.tag.scss",
				"entity.tag.apacheconf"
			],
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#986801"
			}
		},
		{
			"name": "CSS / SCSS / HTML CSS attribute name",
