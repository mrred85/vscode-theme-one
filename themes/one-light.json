		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#986801"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#e06c75"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#a626a4"
			}
